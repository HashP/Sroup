<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 <typeAliases>
   		<typeAlias type="com.cj.sroup.vo.M_galleryVO" alias="m_gallery"/>   		
   		<typeAlias type="com.cj.sroup.vo.M_boardVO" alias="m_board"/> 
   		<typeAlias type="com.cj.sroup.vo.M_commentVO" alias="m_comment" />   		
   		<typeAlias type="com.cj.sroup.vo.UserInfoVO" alias="UserInfo"/>   		
   </typeAliases>
  <!-- SQL이 정의된 mapper파일을 등록한다. -->
  <mappers>
    <mapper resource="META-INF/mybatis/m_gallery.xml"/>
    <mapper resource="META-INF/mybatis/m_board.xml" /> 
    <mapper resource="META-INF/mybatis/m_comment.xml" /> 
    <mapper resource="META-INF/mybatis/userinfo.xml"/>
  </mappers>
</configuration>